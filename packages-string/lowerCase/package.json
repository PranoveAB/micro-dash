{
  "name": "@micro-ux/micro-dash.lowercase",
  "version": "0.0.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:micro-ux/micro-dash.git",
  "author": "Nischith BM <nischithbm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:prod",
    "build:prod": "yarn build:prod:web && yarn build:prod:node && yarn build:types && yarn bundlesize",
    "build:prod:web": "WP_CLEAN=true WP_TARGET=web webpack",
    "build:prod:node": "WP_TARGET=node WP_TARGET_FILENAME=node/[name].js webpack",
    "build:types": "yarn build:types:web && yarn build:types:node",
    "build:types:web": "tsc --emitDeclarationOnly",
    "build:types:node": "tsc --emitDeclarationOnly --outDir dist/node",
    "test": "jest",
    "bundlesize": "bundlesize"
  },
  "dependencies": {
    "@micro-ux/micro-dash-internal-shared": "0.0.4"
  },
  "devDependencies": {
    "@micro-ux/micro-dash-build-config": "0.0.4"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
